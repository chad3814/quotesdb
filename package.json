{
  "name": "quotesdb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "coordinator:dev": "tsx watch src/coordinator/server.ts",
    "coordinator:build": "tsc src/coordinator/server.ts --outDir dist/coordinator",
    "coordinator:start": "node dist/coordinator/server.js",
    "runner:example": "tsx src/runners/exampleRunner.ts",
    "runner:imdb": "tsx src/runners/unifiedImdbRunner.ts tabstack",
    "runner:imdb:tabstack": "tsx src/runners/unifiedImdbRunner.ts tabstack",
    "runner:imdb:playwright": "tsx src/runners/unifiedImdbRunner.ts playwright"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.1.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "next": "15.4.3",
    "next-auth": "^5.0.0-beta.29",
    "playwright": "^1.55.0",
    "prisma": "^6.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/jest": "^29.5.0",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.4",
    "typescript": "^5"
  }
}
